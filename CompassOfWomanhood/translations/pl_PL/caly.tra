@105 = ~Cztery razy po dwa razy to jednak za dużo.~ [6WCALY05]
@106 = ~No chodź, chcę się zabaaawić.~ [6WCALY06]
@107 = ~No chodź na kolanko!~ [6WCALY07]
@108 = ~Przygotuj się na ból... i ociupinkę przyjemności.~ [6WCALY08]
@109 = ~Skosztuj mego bicza!~ [6WCALY09]
@110 = ~To nie było miłe!~ [6WCALY10]
@111 = ~A więc tak wygląda „duża śmierć”...~ [6WCALY11]
@112 = ~Uuh, błoto, mokro... uuuh, ROBACTWO!~ [6WCALY12]
@113 = ~Nareszcie, prawie jak w domu!~ [6WCALY13]
@114 = ~Uuh, błoto. Uuh, mokro... uuuh, GRYZONIE!~ [6WCALY14]
@115 = ~Nowy dzień, nowa ja, nowa baśń!~ [6WCALY15]
@116 = ~Daj mi tę noc, jak mówił pewien rashemeński bard.~ [6WCALY16]
@117 = ~Tak, ssskarbie?~ [6WCALY17]
@118 = ~Masz coś na myśli?~ [6WCALY18]
@119 = ~Tak, kotku?~
@120 = ~Mmmmmm...~ [6WCALY20]
@121 = ~Czuję się wybrana.~ [6WCALY21]
@122 = ~Potrzebujesz moich usług?~
@123 = ~Tak jessst.~
